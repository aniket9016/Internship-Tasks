@model tempLINQ.Models.EmpModel
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/jqueryval")
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <h4>EMPLOYEE</h4>
        <hr />
        <div class="form-group">
            <div class="control-label col-md-2">
                Enter Employee Name
            </div>
            <div class="col-md-10">
                @Html.EditorFor(model => model.name, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">
                Enter Salary
            </div>
            <div class="col-md-10">
                @Html.EditorFor(model => model.sal, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">
                Enter Employee City
            </div>
            <div class="col-md-10">
                @Html.DropDownListFor(model => model.ct, new SelectList(new[] { "Surat", "Vapi", "Valsad", "Mumbai", "Gandhinagar" }), "Select City", new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            <div class="control-label col-md-2">
                Enter Employee Gender
            </div>
            <div class="col-md-10">
                <label class="radio-inline">
                    @Html.RadioButtonFor(model => model.gen, "Male", new { @checked = "checked" }) Male
                </label>
                <label class="radio-inline">
                    @Html.RadioButtonFor(model => model.gen, "Female") Female
                </label>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
}
<div>
    @Html.ActionLink("Back to list", "Index")
</div>
